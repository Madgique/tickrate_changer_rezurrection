{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_display_name}",
  "description": "${mod_description}",
  "authors": ["${mod_authors}"],
  "contact": {
    "homepage": "${mod_public_url}",
    "issues": "${mod_issue_tracker_url}",
    "sources": "${mod_sources}"
  },
  "license": "${mod_license}",
  "icon": "assets/${mod_id}/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "${maven_group}.fabric.${mod_class_name}Fabric"
    ],
    "client": [
      "${maven_group}.fabric.client.${mod_class_name}FabricClient"
    ],
    "modmenu": [
      "${maven_group}.fabric.ClothConfigFabric"
    ]
  },
  "mixins": [
    "${mixin_files}"
  ],
  "depends": {
    "fabricloader": ">=0.11.6",
    "minecraft": "~1.18.2",
    "java": ">=8",
    "architectury": ">=1.7.117",
    "fabric": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}
